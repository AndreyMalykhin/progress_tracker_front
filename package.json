{
  "name": "progress_tracker_front",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "@types/": "yfuks/react-native-action-sheet",
    "@types/debug": "^0.0.30",
    "@types/enzyme": "^3.1.6",
    "@types/faker": "^4.1.2",
    "@types/graphql": "^0.12.4",
    "@types/jest": "^22.0.0",
    "@types/lodash": "^4.14.91",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.31",
    "@types/react-intl": "^2.3.3",
    "@types/react-native": "^0.51.7",
    "@types/react-native-fbsdk": "^0.6.0",
    "@types/react-native-fs": "^2.8.1",
    "@types/react-native-vector-icons": "^4.4.2",
    "@types/react-router-native": "^4.0.2",
    "@types/react-test-renderer": "^16.0.0",
    "@types/uuid": "^3.4.3",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.2.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "env-cmd": "^7.0.0",
    "enzyme": "^3.3.0",
    "husky": "^0.14.3",
    "jest-expo": "23.0.0",
    "lint-staged": "^6.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-native-cli": "^2.0.1",
    "react-test-renderer": "16.0.0",
    "reactotron-react-native": "^1.14.0",
    "ts-jest": "^22.0.0",
    "ts-loader": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^3.0.4",
    "tslint": "^5.8.0",
    "tslint-react": "^3.3.3",
    "typescript": "^2.6.2",
    "webpack": "^4.7.0",
    "webpack-cli": "^2.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "start": "env-cmd ./.env.local react-native start",
    "android": "react-native run-android",
    "ios-simulator": "react-native run-ios --simulator=\"iPhone X\"",
    "ios-simulator-prod": "react-native run-ios --simulator=\"iPhone X\" --configuration=Release",
    "ios-device": "react-native run-ios --device",
    "ios-device-prod": "react-native run-ios --device --configuration=Release",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "dev": "yarn extract-fragment-types && env-cmd ./.env.local webpack --watch --display-modules",
    "build": "yarn extract-fragment-types && env-cmd ./.env.local webpack --display-modules",
    "build-prod": "yarn extract-fragment-types-prod && env-cmd ./.env.production.local webpack --display-modules",
    "fastlane-prod": "cd ios && env-cmd ./.env.production.local bundle exec fastlane",
    "precommit": "lint-staged",
    "extract-fragment-types": "env-cmd ./.env.local node src/scripts/extract-fragment-types.js",
    "extract-fragment-types-prod": "env-cmd ./.env.production.local node src/scripts/extract-fragment-types.js"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ]
  },
  "jest": {
    "preset": "react-native",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "android.js",
      "android.ts",
      "android.tsx",
      "ios.js",
      "ios.ts",
      "ios.tsx"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/build/"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "dependencies": {
    "@yfuks/react-native-action-sheet": "^0.0.4",
    "apollo-cache-inmemory": "^1.1.4",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.0.4",
    "apollo-link": "^1.0.7",
    "apollo-link-context": "^1.0.5",
    "apollo-link-error": "^1.0.5",
    "apollo-link-http": "^1.3.2",
    "apollo-link-http-common": "^0.2.3",
    "apollo-link-logger": "^1.2.0",
    "apollo-link-state": "^0.3.0",
    "bottlejs": "^1.7.0",
    "debug": "^3.1.0",
    "faker": "^4.1.0",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "intl": "^1.2.5",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "16.0.0",
    "react-apollo": "^2.0.4",
    "react-intl": "^2.4.0",
    "react-native": "0.51.0",
    "react-native-animatable": "^1.2.4",
    "react-native-device-info": "^0.13.0",
    "react-native-fbsdk": "^0.7.0",
    "react-native-fs": "^2.9.11",
    "react-native-image-crop-picker": "^0.19.1",
    "react-native-image-progress": "^1.0.1",
    "react-native-keyboard-aware-scroll-view": "^0.4.3",
    "react-native-modal": "^4.1.1",
    "react-native-modal-datetime-picker": "^4.13.0",
    "react-native-progress": "^3.4.0",
    "react-native-sentry": "^0.35.3",
    "react-native-sound": "^0.10.8",
    "react-native-splash-screen": "^3.0.6",
    "react-native-typography": "^1.3.0",
    "react-native-vector-icons": "^4.4.3",
    "react-router-native": "^4.2.0",
    "redux-persist-fs-storage": "^1.2.1",
    "uuid": "^3.1.0"
  }
}
